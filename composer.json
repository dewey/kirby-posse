{
    "name": "notmyhostname/posse",
    "description": "POSSE plugin for Kirby CMS",
    "type": "kirby-plugin",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Philipp Defner",
            "email": "mail@notmyhostna.me"
        }
    ],
    "require": {
        "getkirby/composer-installer": "^1.1",
        "cjrasmussen/bluesky-api": "^2.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10"
    },
    "scripts": {
        "analyze": "phpstan analyse"
    },
    "extra": {
        "installer-name": "posse"
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "getkirby/composer-installer": true
        }
    }
}
